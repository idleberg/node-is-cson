{
  "name": "is-cson",
  "version": "3.0.2",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "esbuild src/is-cson.ts --platform=node --outfile=dist/is-cson.cjs --format=cjs",
    "build:esm": "esbuild src/is-cson.ts --platform=node --outfile=dist/is-cson.mjs --format=esm",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint:json": "eslint ./*.json --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "npm run build:esm -- --watch",
    "test": "uvu tests",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "type": "module",
  "exports": {
    "import": "./dist/is-cson.mjs",
    "require": "./dist/is-cson.cjs"
  },
  "typings": "types/",
  "files": [
    "dist/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "dependencies": {
    "cson-parser": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.16",
    "eslint": "^8.45.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all2": "^6.0.6",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix"
  }
}
