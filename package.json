{
  "name": "is-cson",
  "version": "1.1.1",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "start": "rollup --watch --config",
    "test": "ava --verbose"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "cson-parser": "^4.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "ava": "^3.12.1",
    "coffeescript": "^2.5.1",
    "eslint": "^7.8.1",
    "esm": "^3.2.25",
    "husky": "4.2.5",
    "rollup": "^2.26.10",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "typescript": "^4.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
