{
  "name": "is-cson",
  "version": "1.1.1",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "npm run start",
    "lint": "tslint --project .",
    "start": "tsc --watch --pretty --project ./",
    "test": "ava --verbose"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "cson-parser": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "ava": "^3.12.1",
    "esm": "^3.2.25",
    "husky": "4.2.5",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
