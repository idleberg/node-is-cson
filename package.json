{
  "name": "is-cson",
  "version": "1.0.0",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "test": "npx ava --verbose"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "cson-parser": "^4.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "ava": "^2.4.0",
    "husky": "4.2.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  }
}
