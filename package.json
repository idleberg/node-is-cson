{
  "name": "is-cson",
  "version": "0.1.1",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "test": "npx ava --verbose"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "cson-parser": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "ava": "^2.4.0",
    "husky": "3.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "util": "^0.12.1"
  }
}
