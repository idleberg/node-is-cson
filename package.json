{
  "name": "is-cson",
  "version": "0.1.0",
  "description": "Determines whether a string is valid CSON",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project .",
    "test": "npx ava ./test/ --verbose"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-is-cson.git"
  },
  "keywords": [
    "cson",
    "coffeescript"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "cson-parser": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "ava": "^1.4.1",
    "husky": "2.3.0",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.4000",
    "util": "^0.12.0"
  }
}
